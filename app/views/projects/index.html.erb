<h1>Projects List</h1>

<%= link_to "New Project", new_project_path %>

<br /><br />

<table>
  <thead>
    <th>Project Name</th>
    <td></td>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <%= content_tag_for :tr, project do %>
        <td><%= link_to project.name, project_stories_path(project) %></td>
        <td><%= button_to 'Edit Project', edit_project_path(project), :method => :get %></td>
      <% end %>
    <% end %>
  </tbody>
</table>



